; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Quasar Framework"
run/main_scene="uid://cp10wwskfbdoy"
config/features=PackedStringArray("4.4")
run/max_fps=120
config/icon="res://icon.svg"

[autoload]

MenuConductor="*res://scripts/Conductor.gd"
SceneLoader="*res://autoload/scene_loader.gd"
UserSettings="*res://autoload/user_settings.gd"
VolumeController="*res://autoload/volume_controller.tscn"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/size/sharp_corners=true
window/stretch/mode="canvas_items"
window/stretch/aspect="keep_height"

[dotnet]

project/assembly_name="Quasar Framework"

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[editor_plugins]

enabled=PackedStringArray("res://addons/fancy_editor_sounds/plugin.cfg")

[file_customization]

folder_colors={
"res://autoload/": "gray",
"res://resources/": "purple",
"res://scenes/": "pink",
"res://scripts/": "teal"
}

